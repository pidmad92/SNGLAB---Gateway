        <addForeignKeyConstraint baseColumnNames="tipproveid_id"
                                 baseTableName="docexpedien"
                                 constraintName="fk_docexpedien_tipproveid_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tipproveid"/>

    </changeSet>
</databaseChangeLog>
